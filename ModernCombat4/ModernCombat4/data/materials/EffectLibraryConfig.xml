<?xml version="1.0" encoding="utf-8"?>
<!-- Users can edit the values in this file to customize the effect library. -->
<config>
<!-- Do not modify the version number. -->
  <version>0.0.0.1</version>
  <render_states>
<!-- If set to true, you need to define GLITCH_HAS_SEPARATE_BLENDING in your glitch compile config file. -->
    <blend_operations_separate>false</blend_operations_separate>
<!-- If set to true, you need to define GLITCH_HAS_SEPARATE_STENCIL in your glitch compile config file. -->
    <stencil_operations_separate>false</stencil_operations_separate>
  </render_states>
  <profiles>
<!--List of profiles exposed to the user.
It is strongly recommended to minimize the number of profiles exposed to the user.
However there must be at least one profile in the list.
Valid Profiles : GLES_1_1 GLES_2_0 GLSL CG -->
    <available_profiles>
      <profile name="GLES_1_1">
        <platforms>
          <platform name="HighEnd"/>
		  <platform name="LowEnd"/>
        </platforms>
      </profile>
      <profile name="GLES_2_0">
        <platforms>
          <platform name="HighEnd"/>
		  <platform name="LowEnd"/>
        </platforms>
      </profile>
      <profile name="GLSL">
        <platforms>
          <platform name="HighEnd"/>
		  <platform name="LowEnd"/>
        </platforms>
      </profile>
      <profile name="CG">
        <platforms>
          <platform name="HighEnd"/>
		  <platform name="LowEnd"/>
        </platforms>
      </profile>
    </available_profiles>
  </profiles>
</config>

